<table cellpadding="0" cellspacing="0" border="0" class="display table table-bordered dataTables_wrapper" id="accounts" role="grid" data-source="<%= accounts_path(format: :json) %>">
	<thead>
		<tr>
			<th>Date</th>
			<th>Name</th>
			<th>Vehicle Box</th>
			<th>Vehicle Kg</th>
			<th>Rate</th>
			<th>Feed</th>
			<th>Amount</th>
			<th>Paid</th>
			<th>Balance</th>
			<th>Old Balance</th>
			<th>Total Balance</th>
			<th>Total Paid</th>
			<th>Total Amount</th>
			<th>Actions</th>
		</tr>
	</thead>
	<tbody>
	</tbody>
</table>
<script type="text/javascript">
	$('#accounts').dataTable( {
		paginationType: "full_numbers",
		processing: true,
		serverSide: true,
		ajax: {
			url: $('#accounts').data('source'),
			method: 'POST'
		},
		iDisplayLength: 5
	});
	
	$( "#sales_calender_date" ).datepicker({
  	dateFormat: 'dd/mm/yy'
  });
</script>