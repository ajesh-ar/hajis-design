<table cellpadding="0" cellspacing="0" border="0" class="display table table-bordered dataTables_wrapper" id="user-profile" role="grid">
	<thead>
		<tr>
			<th>Resource Type</th>
			<th>Event</th>
			<th>Who Did?</th>
			<th>Change</th>
			<!-- <th>Date</th> -->
		</tr>
	</thead>
	<tbody>
		<% @customer_versions.each do |version| %>
			<tr>
				<td><%= version.item_type %></td>
				<td><%= version.event %></td>
				<td><%= User.find(version.whodunnit).email rescue 'Unknown' %></td>
				<td><%= version.object.match(/old_balance: (.*)/)[0] %></td>
				<!-- <td><%= version.object.match(/updated_at: (.*)/)[0] %></td> -->
			</tr>
		<% end %>
	</tbody>
</table>
<script type="text/javascript">
$('#user-profile').dataTable( {
	// jQueryUI: true,
	paginationType: "full_numbers"
});
</script>